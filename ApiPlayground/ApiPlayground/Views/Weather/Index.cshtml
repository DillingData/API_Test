@model IEnumerable<WeatherModel>


@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p id="Test">Loading...</p>

<input type="button" class="btn btn-primary" onclick="location.href='@Url.Action("OnGet", "Weather")'" Testing />

@if (Model.Count() > 0)
{
    @foreach (WeatherModel temp in Model)
    {
        <p>@temp.main.temp</p>
    }
}
else
{
    <p>Loading...</p>
}

<script src="~/js/Weather.js" asp-append-version="true"></script>